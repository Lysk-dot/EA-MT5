
# Infra env example (customize for production)
POSTGRES_USER=ea
# Use a strong password, at least 16 chars, random
POSTGRES_PASSWORD=REPLACE_ME_WITH_STRONG_SECRET
POSTGRES_DB=ea
POSTGRES_PORT=5432
APP_PORT=18001
# Use a strong token, at least 32 chars, random
ALLOWED_TOKEN=REPLACE_ME_WITH_STRONG_TOKEN
FORWARD_INGEST_URL=
FORWARD_TICK_URL=
FORWARD_TOKEN=
FORWARD_CONFIRM_URL=
# Use a dedicated email and strong password for PgAdmin
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=REPLACE_ME_WITH_STRONG_SECRET
PGADMIN_PORT=18003

# Security tips:
# - Never commit real secrets to git
# - Rotate tokens and passwords regularly
# - Restrict API port (APP_PORT) to trusted sources only
# - Keep Prometheus/Grafana/PgAdmin internal or behind VPN/reverse proxy

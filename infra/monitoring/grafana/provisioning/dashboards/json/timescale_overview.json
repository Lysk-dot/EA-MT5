{
  "title": "Timescale Overview",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "Conexões Postgres",
      "gridPos": {"x":0, "y":0, "w":12, "h":6},
      "targets": [{
        "expr": "pg_stat_activity_count{datname=\"ea\"}",
        "legendFormat": "conexões"
      }]
    },
    {
      "type": "graph",
      "title": "Tuplas inseridas/s",
      "gridPos": {"x":0, "y":6, "w":12, "h":6},
      "targets": [{
        "expr": "rate(pg_stat_database_tup_inserted{datname=\"ea\"}[1m])",
        "legendFormat": "insert/s"
      }]
    },
    {
      "type": "graph",
      "title": "Tempo de consulta (se houver histograms)",
      "gridPos": {"x":0, "y":12, "w":12, "h":6},
      "targets": [{
        "expr": "pg_stat_statements_mean_time_seconds",
        "legendFormat": "mean"
      }],
      "hiddenSeries": true
    }
  ]
}
